{
    "preset": "laravel",
    "rules": {
        "not_operator_with_space": false,
        "single_line_empty_body": false,
        "nullable_type_declaration_for_default_null_value": true,
        "phpdoc_align": {
            "align": "left"
        },
        "concat_space": {
            "spacing": "one"
        },
        "new_with_parentheses": false,
        "statement_indentation": false
    }
}
